{
  "searchPaths": [
    [
      "core",
      "target",
      "devon-core-1.0-SNAPSHOT.jar"
    ]
  ],
  "scrubGroupingPattern": "com/seanshubin/([^/]+).*",
  "styleSheet": [
    "dependency.css"
  ],
  "relevantDependencyPattern": {
    "includeRegex": [
      "com/seanshubin/devon/.*"
    ],
    "excludeRegex": []
  },
  "trimRegex": "^com/seanshubin/devon/",
  "classFileNamePattern": {
    "includeRegex": [
      ".*\\.class"
    ],
    "excludeRegex": []
  },
  "zipFileNamePattern": {
    "includeRegex": [
      ".*\\.zip",
      ".*\\.war",
      ".*\\.jar"
    ],
    "excludeRegex": []
  },
  "generatedReportDirectory": [
    "core",
    "target",
    "dependency-report"
  ],
  "classLimits": {
    "depth": 8,
    "complexity": 100,
    "cycleCount": 0,
    "cycleSize": 0
  },
  "packageLimits": {
    "depth": 4,
    "complexity": 10,
    "cycleCount": 0,
    "cycleSize": 0
  },
  "groupingLimits": {
    "depth": 1,
    "complexity": 0,
    "cycleCount": 0,
    "cycleSize": 0
  },
  "failIfAnyLimitExceeded": true
}
